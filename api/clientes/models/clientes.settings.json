{
  "kind": "collectionType",
  "collectionName": "clientes",
  "info": {
    "name": "clientes",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nombres": {
      "type": "string",
      "required": true
    },
    "apellidos": {
      "type": "string",
      "required": true
    },
    "ciudad": {
      "type": "string",
      "required": true
    },
    "direccion": {
      "type": "string",
      "required": true
    },
    "correo": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "estado": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "documentos_de_clientes": {
      "via": "cliente",
      "collection": "documentos-de-clientes"
    },
    "cedula": {
      "type": "string",
      "maxLength": 10,
      "required": true
    },
    "solicitud_de_creditos": {
      "via": "cliente",
      "collection": "solicitud-de-credito"
    },
    "celular": {
      "type": "string"
    },
    "telefono": {
      "type": "string"
    },
    "contrasenia": {
      "type": "string",
      "required": true
    },
    "foto": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "cedula_personal": {
      "model": "file",
