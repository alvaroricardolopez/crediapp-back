      "required": false,
      "pluginOptions": {}
    },
    "cedula_conyugue": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
